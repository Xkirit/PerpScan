{
  "name": "bybit-analyzer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:verbose": "NODE_ENV=development next dev --turbopack",
    "dev:silent": "NEXT_TELEMETRY_DISABLED=1 next dev --turbopack --quiet > /dev/null 2>&1",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test-cron": "curl -s https://perpflow.vercel.app/api/institutional-flows && echo '\\nCron test completed'",
    "upstash-info": "curl -s http://localhost:3000/api/redis-info | jq '.redis'",
    "upstash-info-full": "curl -s http://localhost:3000/api/redis-info | jq '.'",
    "upstash-debug": "curl -s http://localhost:3000/api/debug-redis | jq -r '.capturedLogs[]'",
    "upstash-debug-full": "curl -s http://localhost:3000/api/debug-redis | jq '.'",
    "keep-warm": "node keep-warm.js",
    "keep-warm:loop": "while true; do npm run keep-warm; sleep 300; done",
    "clean-privacy": "./scripts/clean-privacy.sh",
    "deploy-clean": "npm run clean-privacy && git add . && git commit -m 'Clean privacy deployment' && git push"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@upstash/redis": "^1.34.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/edge-config": "^1.4.0",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gsap": "^3.13.0",
    "liquid-glass-react": "^1.1.1",
    "lucide-react": "^0.514.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
